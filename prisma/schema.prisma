// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id        String @id @default(uuid())
  name      String
  email     String @unique
  password  String
  nickname  String
  img_user  String
  street    String
  number    Int
  city      String
  state     String
  zip_code  String
  reference String

  @@map("users")
}
